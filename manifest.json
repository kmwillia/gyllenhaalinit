{
    "manifest_version": 2,
    "name": "Gyllenhaalin' it",
    "version": "1.0.0",
    "description": "Replaces all misspellings (neanderthal) of Jake Gyllenhaal's name with the correct spelling.  Can also replace all images with images of Jake.",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/main.js"],
            "run_at": "document_end"
        }
    ],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [
        "rsc/pics/*"
    ]
}
