{
    "manifest_version": 2,
    "name": "Gyllenhaalin' it",
    "version": "1.0.2",
    "description": "Replaces the term Neaderthal with Jake Gyllenhaal.  Can also replace all images with images of Jake.",
    "homepage_url": "https://github.com/kmwillia/gyllenhaalinit",
    "icons": {
        "16": "icons/JG-16.png",
        "32": "icons/JG-32.png",
        "48": "icons/JG-48.png",
        "128": "icons/JG-128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/main.js"],
            "run_at": "document_end"
        }
    ],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [
        "rsc/pics/*"
    ]
}
